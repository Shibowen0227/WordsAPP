<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.edu.bistu.cs.se.mywordsapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.Shibunny.shi.bo.wen.wordsAPP.WordsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.Shibunny.shi.bo.wen.wordsAPP.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Shibunny.shi.bo.wen.wordsAPP.WordDetailActivity" />
        <activity
            android:name="com.Shibunny.shi.bo.wen.wordsAPP.MainActivity_2"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.Shibunny.shi.bo.wen.wordsAPP.English"
            android:label=".english"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>



